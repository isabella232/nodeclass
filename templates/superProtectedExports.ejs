<% if ($superProperties) { %>
    <% _($superProperties.protected.attribute).each(function printProtectedSuperSetterGetter(value, key) { %>
        <% var setterKey = $nameModifiers.setter(key); %>
        <% var getterKey = $nameModifiers.getter(key); %>
this._<%= setterKey %> = Super._<%= setterKey %>;
this._<%= getterKey %> = Super._<%= getterKey %>;
    <% }); %>
    <% _($superProperties.protected.method).each(function printProtectedSuperMethods(value, key) { %>
this.<%= key %> = Super.<%= key %>;
    <% }); %>
<% } %>